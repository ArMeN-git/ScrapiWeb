@model System.Web.Mvc.HandleErrorInfo
@using WebScrapingUI.Resources;

@{
    ViewBag.Title = Account.Error;
}

<h1 class="text-danger">@ViewBag.Title</h1>
@{
    if (String.IsNullOrEmpty(ViewBag.errorMessage))
    {
        <h2 class="text-danger">@Account.ErrorOccured</h2>
    }
    else
    {
        <h2 class="text-danger">@ViewBag.errorMessage</h2>
    }
}